reg [12:0] countC;	
reg [2:0] Cstate;		

//assign	LED_GREEN	=	VGA_Y;
//assign	LED_RED		=	VGA_X;

assign	LED_GREEN	=	flag;
assign	LED_RED		=	countC;
				
always @ (posedge VGA_CLK)
begin
	if(VGA_X < 10'd39 && VGA_Y > 9'd360)
		begin
			if(VGA_Y < 9'd478)
				begin
					if(mGreen > 10'h99 && mRed < 10'h80 && mBlue < 10'h80)
						begin
							countC <= countC + 1;
						end
				end
			else //if(VGA_Y > 9'd477)
						begin
							case (Cstate)
							0:
							begin
								if(countC > 11'd120) // Green present, play string
									begin
										if(!pre_flag && !plucked)
											begin
												flag <= 1;
												playC <= 1;
											end
										else
											begin
												flag <= 1;
												if(!plucked)
													playC <= 0;
											end
									end
								else // Green not present, do not play string
									begin
										flag <= 0;
										if(!plucked)
											playC <= 0;
									end
								Cstate <= 1;
							end
							1:
							begin
								pre_flag <= flag;
								countC <= 0;
								Cstate <= 0;
							end
							endcase
						end
		end
end
	
